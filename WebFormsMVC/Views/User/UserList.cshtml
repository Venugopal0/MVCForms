@model List<WebFormsMVC.Models.User>

@{
    // var users = TempData["Users"] as IEnumerable<User>;
}
<html>
<head>
    <title>UserList</title>
</head>
<body>
    <table>
        <tr>
            <th>UserId</th>
            <th>UserName</th>
            <th>Email</th>
        </tr>
        @foreach (var user in Model)
        {
            
            <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
                <td>@user.Email</td>
                <td>  @Html.ActionLink("Edit", "Edit", new { id = user.Id })  </td>
                <td>  @Html.ActionLink("Delete", "Delete", new { id = user.Id })</td>

            </tr>
        }
    </table>

</body>
</html>